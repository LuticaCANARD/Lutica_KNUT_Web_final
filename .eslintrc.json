{
	"env": {
		"browser": true,
		"es2021": true
	},
	"extends": [
		"eslint:recommended"
		],
	"parserOptions": {
		"ecmaVersion": "latest",
		"sourceType": "module"
	},
	"rules": {
		"indent": [
			"error",
			"tab"
		],
		"semi": [
			"error",
			"always"
		],
		"no-unused-vars": ["warn", { "vars": "all", "args": "after-used", "ignoreRestSiblings": false }]

	}
}
